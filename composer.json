{
	"name": "serato/sws-php-sdk",
	"homepage": "https://github.com/serato/sws-php-sdk",
	"description": "PHP SDK for Serato Web Services",
	"type": "library",
	"require": {
		"php": "^7.1 || ^8.0",
		"guzzlehttp/guzzle": "^6.0.0",
		"serato/sws-discovery": "dev-TDO-789"
	},
	"require-dev": {
		"phpunit/phpunit": "^7 || ^8",
		"squizlabs/php_codesniffer": "^3.0",
		"aws/aws-sdk-php": "^3.0",
		"phpstan/phpstan": "^1.4",
		"phpstan/phpstan-phpunit": "^1.0",
		"enlightn/security-checker": "^1.4 || ^2"
	},
	"autoload": {
		"psr-4": {
			"Serato\\SwsSdk\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\SwsSdk\\Test\\": "tests/"
		}
	}
}
