{
	"name": "serato/sws-php-sdk",
	"homepage": "https://github.com/serato/sws-php-sdk",
	"description": "PHP SDK for Serato Web Services",
	"type": "library",
	"require": {
		"php": "^7.1",
		"guzzlehttp/guzzle": "^6.0.0",
		"serato/sws-discovery": "^2.0.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~7.0",
		"squizlabs/php_codesniffer": "~3.6",
		"aws/aws-sdk-php": "~3.0",
		"phpstan/phpstan": "^0.12.0",
		"phpstan/phpstan-phpunit": "^0.12.0",
		"enlightn/security-checker": "^1.4"
	},
	"autoload": {
		"psr-4": {
			"Serato\\SwsSdk\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Serato\\SwsSdk\\Test\\": "tests/"
		}
	}
}
